{
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "import"
  ],
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "comma-dangle": ["error", "never"],
    "import/prefer-default-export": "off",
    "function-paren-newline": "off",
    "lines-between-class-members": "off"
  },
  "globals": {
    "$": true,
    "moment": true,
    "Dropzone": true,
    "document": true,
    "fetch": true,
    "localStorage": true,
    "window": true
  }
}
